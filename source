local tab = win:Tab("Boss Farm", "http://www.roblox.com/asset/?id=6023426945")

tab:Toggle("AncientMagmaBoss"," ",false,function(t)
    
_G.A = t 
if _G.A == false then return end
while _G.A do wait()
for i,v in pairs(game:GetService("Workspace").bossFolder:GetChildren()) do
    if v.Name == "AncientMagmaBoss" then
        repeat
            VirtualUser:CaptureController()
            VirtualUser:ClickButton1(Vector2.new(50, 50), CFrame.new(Vector3.new(0, 0, 0)))
            wait()
            plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,0)
            until v.Humanoid.Health <= 0 or _G.A == false
        end
end
end
end)

tab:Line()

tab:Toggle("EternalBoss","",false,function(t)
    
    _G.A = t 
    if _G.A == false then return end
    while _G.A do wait()
    
    for i,v in pairs(game:GetService("Workspace").bossFolder:GetChildren()) do
        if v.Name == "EternalBoss" then
            repeat
                VirtualUser:CaptureController()
                VirtualUser:ClickButton1(Vector2.new(50, 50), CFrame.new(Vector3.new(0, 0, 0)))
                wait()
                plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,0)
                until v.Humanoid.Health <= 0 or _G.A == false
            end
    end
    end
end)

tab:Line()

    tab:Toggle("RobotBoss"," ",false,function(t)
    
    _G.A = t 
    if _G.A == false then return end
    while _G.A do wait()
        
    for i,v in pairs(game:GetService("Workspace").bossFolder:GetChildren()) do
        if v.Name == "RobotBoss" then
            repeat
                VirtualUser:CaptureController()
                VirtualUser:ClickButton1(Vector2.new(50, 50), CFrame.new(Vector3.new(0, 0, 0)))
                wait()
                plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,0)
                until v.Humanoid.Health <= 0 or _G.A == false
            end
    end
    end
end)
    

local tab = win:Tab("Teleport", "http://www.roblox.com/asset/?id=6023426945")

tab:Button("Teleport All Islands", "Teleport All Islands",function()
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Enchanted Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Astral Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Mystical Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Space Island"].islandSignPart.CFrame
    wait(1.5)
