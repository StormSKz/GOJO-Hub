    self.CFrame = game:GetService("Workspace").islandUnlockParts["Skystorm Ultraus Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Soul Fusion Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Dark Elements Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Inner Peace Island"].islandSignPart.CFrame
    wait(1.5)
end)

