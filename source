    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Sandstorm"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Sandstormisland"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Ancient Inferno Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Midnight Shadow Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Mythical Souls Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Winter Wonder Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Golden Master Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Dragon Legend Island"].islandSignPart.CFrame
    wait(1.5)
    self.CFrame = game:GetService("Workspace").islandUnlockParts["Cybernetic Legends Island"].islandSignPart.CFrame
