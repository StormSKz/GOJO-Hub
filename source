local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()

local win = Flux:Window("Ninja Legends", "GOJO Hub", Color3.fromRGB(0, 0, 255), Enum.KeyCode.RightControl)
local tab = win:Tab("MainFarm", "http://www.roblox.com/asset/?id=6023426945")

local self = game.Players.LocalPlayer.Character.HumanoidRootPart
local plr = game.Players.LocalPlayer
local VirtualUser = game:GetService("VirtualUser")

tab:Label("Ninja Legends Auto Farm")

tab:Toggle("Auto swings", "Auto swings", false, function(t)

_G.A = t 
if _G.A == false then return end
while _G.A do wait(0.00001)

    local string_1 = "swingKatana";
local Target = game:GetService("Players").LocalPlayer.ninjaEvent;
Target:FireServer(string_1);
wait(0.00001)
end
end)

tab:Toggle("Auto Sell", "Auto sells Ninjitsu", false, function(t)
    _G.A = t 
if _G.A == false then return end
while _G.A do wait(0.00001)

        if game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart") then
        game.workspace.sellAreaCircles["sellAreaCircle16"].circleInner.CFrame = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
        wait(0.00001)
        game.workspace.sellAreaCircles["sellAreaCircle16"].circleInner.CFrame = game.Workspace.Part.CFrame
        end
        end
end)
    
tab:Line()
tab:Label("Auto collects")

tab:Toggle("Auto collect Chi"," ",false,function(t)

_G.A = t 
if _G.A == false then return end
while _G.A do wait()
    self.CFrame = game:GetService("Workspace").spawnedCoins.Valley["Blue Chi Crate"].CFrame
    wait()
end
end)

tab:Toggle("Auto collect Coins"," ",false,function(t)

    _G.A = t 
    if _G.A == false then return end
    while _G.A do wait()
        self.CFrame = game:GetService("Workspace").spawnedCoins.Valley["Purple Coin Crate"].CFrame
        wait()
    end
end)

tab:Line()
tab:Label("Auto Upgrades")

tab:Toggle("Auto Upgrades Swords", "Auto Buy Swords", false, function(t)

    _G.A = t 
    if _G.A == false then return end
    while _G.A do wait()

        local string_1 = "buyAllSwords";
local string_2 = "Inner Peace Island";
local Target = game:GetService("Players")["LocalPlayer"].ninjaEvent;
Target:FireServer(string_1, string_2);
end
end)

tab:Toggle("Auto Upgrades Belts", "Auto Buy Belts", false, function(t)
    _G.A = t 
    if _G.A == false then return end
    while _G.A do wait()

        local string_1 = "buyAllBelts";
        local string_2 = "Inner Peace Island";
        local Target = game:GetService("Players")["LocalPlayer"].ninjaEvent;
        Target:FireServer(string_1, string_2);
    end
end)

tab:Toggle("Auto Upgrades Shurikens", "Auto Buy Shurikens", false, function(t)
    _G.A = t 
    if _G.A == false then return end
    while _G.A do wait()

        local string_1 = "buyAllShurikens";
        local string_2 = "Inner Peace Island";
        local Target = game:GetService("Players")["LocalPlayer"].ninjaEvent;
        Target:FireServer(string_1, string_2);
    end
end)

tab:Toggle("Auto Upgrades Ranks", "Auto Buy Ranks", false, function(t)
    _G.A = t 
    if _G.A == false then return end
    while _G.A do wait()

        local string_1 = "buyAllRanks";
        local string_2 = "Inner Peace Island";
        local Target = game:GetService("Players")["LocalPlayer"].ninjaEvent;
        Target:FireServer(string_1, string_2);
    end
end)

tab:Toggle("Auto Upgrades Skills", "Auto Buy skills", false, function(t)
_G.A = t 
if _G.A == false then return end
while _G.A do wait()
    
    local string_1 = "buyAllSkills";
    local string_2 = "Inner Peace Island";
    local Target = game:GetService("Players")["LocalPlayer"].ninjaEvent;
    Target:FireServer(string_1, string_2);
end
